<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="AvaloniaApplication1.MainWindow"
        Title="Array Search" Width="500" Height="500">
    <StackPanel>
        <TextBlock Name="FirstFiveTextBlock" Margin="10"/>
        <TextBox Name="SearchTextBox"  Margin="10"/>
        <ComboBox Name="SearchMethodComboBox" SelectedIndex="1" MaxDropDownHeight="100" Margin="10">
            <ComboBoxItem Content="Sequential Search"/>
            <ComboBoxItem Content="Fibonacci Search"/>
            <ComboBoxItem Content="Interpolation Search"/>
            <ComboBoxItem Content="Hash Search"/>
        </ComboBox>
        <Button Name="SearchButton" Content="Search" Margin="10"/>
        <TextBlock Name="ResultTextBlock" Margin="10"/>
    </StackPanel>
</Window>